<h1>Listing seguros</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @seguros.each do |seguro| %>
  <tr>
    <td><%= seguro.nombre %></td>
    <td><%= link_to 'Show', seguro %></td>
    <td><%= link_to 'Edit', edit_seguro_path(seguro) %></td>
    <td><%= link_to 'Destroy', seguro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Seguro', new_seguro_path %>
